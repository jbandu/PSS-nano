{
  "name": "@airline-ops/mobile-app",
  "version": "1.0.0",
  "description": "Airline Ops Mobile App for iOS and Android",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package",
    "prebuild:ios": "cd ios && pod install",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle",
    "build:android": "cd android && ./gradlew assembleRelease",
    "clean:ios": "cd ios && rm -rf build && pod deintegrate && pod install",
    "clean:android": "cd android && ./gradlew clean",
    "clean": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-camera-roll/camera-roll": "^7.4.0",
    "@react-native-community/netinfo": "^11.2.0",
    "@react-native-firebase/analytics": "^19.0.1",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.1.0",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.5",
    "date-fns": "^3.0.6",
    "i18next": "^23.7.16",
    "lottie-react-native": "^6.5.1",
    "react": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.73.2",
    "react-native-barcode-builder": "^2.0.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^5.4.1",
    "react-native-calendars": "^1.1304.1",
    "react-native-device-info": "^10.13.1",
    "react-native-document-picker": "^9.1.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-permissions": "^4.1.0",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-vision-camera": "^3.8.2",
    "react-native-wallet-manager": "^1.1.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.16.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.4",
    "react-native-dotenv": "^3.4.10",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "resolutions": {
    "@types/react": "^18.2.48"
  }
}
