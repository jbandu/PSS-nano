{
  "name": "@airline-ops/analytics-service",
  "version": "1.0.0",
  "description": "Comprehensive data warehouse and analytics service for airline operations",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "etl:run": "ts-node src/etl/runner.ts",
    "etl:schedule": "node dist/etl/scheduler.js",
    "quality:check": "ts-node src/quality/validator.ts",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seeds/seed.ts",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "analytics",
    "data-warehouse",
    "etl",
    "olap",
    "business-intelligence"
  ],
  "author": "Airline Operations Team",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.8.1",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.5.3",
    "fast-csv": "^5.0.1",
    "exceljs": "^4.4.0",
    "node-fetch": "^3.3.2",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "@types/bull": "^4.10.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "prisma": "^5.8.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  }
}
