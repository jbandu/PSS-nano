{
  "name": "@airline-ops/gds-integration-service",
  "version": "1.0.0",
  "description": "Global Distribution System integration for Amadeus, Sabre, Travelport, and other GDS networks",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:gds": "jest --testPathPattern=gds",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "queue:worker": "ts-node src/workers/message-processor.ts",
    "sync:inventory": "ts-node src/services/sync/inventory-sync.ts",
    "sync:fares": "ts-node src/services/sync/fare-sync.ts",
    "monitor:health": "ts-node src/monitoring/health-check.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.8.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "axios": "^1.6.5",
    "xml2js": "^0.6.2",
    "fast-xml-parser": "^4.3.3",
    "edifact": "^1.2.12",
    "edifact-parser": "^0.0.4",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "amqplib": "^0.10.3",
    "node-cron": "^3.0.3",
    "opossum": "^8.1.2",
    "prom-client": "^15.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "iconv-lite": "^0.6.3",
    "encoding": "^0.1.13",
    "soap": "^1.0.0",
    "luxon": "^3.4.4",
    "lodash": "^4.17.21",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "@types/amqplib": "^0.10.4",
    "@types/lodash": "^4.14.202",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "prisma": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "gds",
    "amadeus",
    "sabre",
    "travelport",
    "airline",
    "distribution",
    "edifact",
    "pnr",
    "booking",
    "ticketing"
  ],
  "author": "Airline Operations Platform",
  "license": "PROPRIETARY"
}
