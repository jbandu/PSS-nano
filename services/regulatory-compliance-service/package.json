{
  "name": "@airline-ops/regulatory-compliance-service",
  "version": "1.0.0",
  "description": "Government regulatory compliance service for TSA Secure Flight, APIS, travel document verification, sanctions screening, privacy, and financial regulations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:compliance": "jest --testPathPattern=compliance",
    "db:generate": "prisma generate",
    "db:push": "prisma db:push",
    "db:migrate": "prisma migrate dev",
    "worker:sfpd": "ts-node src/workers/secure-flight-worker.ts",
    "worker:apis": "ts-node src/workers/apis-worker.ts",
    "worker:sanctions": "ts-node src/workers/sanctions-screening.ts",
    "worker:gdpr": "ts-node src/workers/gdpr-processor.ts",
    "sync:timatic": "ts-node src/services/sync/timatic-sync.ts",
    "sync:sanctions": "ts-node src/services/sync/sanctions-sync.ts",
    "audit:generate": "ts-node src/services/audit/generate-report.ts",
    "monitor:health": "ts-node src/monitoring/compliance-health.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.8.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "axios": "^1.6.5",
    "xml2js": "^0.6.2",
    "fast-xml-parser": "^4.3.3",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "amqplib": "^0.10.3",
    "node-cron": "^3.0.3",
    "opossum": "^8.1.2",
    "prom-client": "^15.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "node-forge": "^1.3.1",
    "jwk-to-pem": "^2.0.5",
    "soap": "^1.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "sanitize-html": "^2.11.0",
    "validator": "^13.11.0",
